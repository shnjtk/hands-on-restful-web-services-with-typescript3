    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "express-winston": "^4.0.1",
    "halson": "^3.0.0",
    "js2xmlparser": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.8",
    "mongoose-unique-validator": "^2.0.3",
    "passport": "^0.4.0",
